public class USACO{
    public int[][] pasture;
    public int[] lakeLevel;

    public void stomp(int r, int c, int d){
	pasture[r][c] = max - d;

	for(int x = r; r < 3; r++){
	    for(int y = c; y < 3; y++){
		if(pasture[x][y] > pasture[r][c]){
		    pasture[x][y] = pasture[r][c];
		}
	    }
	}
    }
    
    public int[] getHighE(int r, int c){
	int max = pasture[r][c] ;
	int row = r;
	int col = c;

	for(int x = r; r < 3; r++){
	    for(int y = c; y < 3; y++){
		if(pasture[x][y] > max){
		    max = pasture[x][y];
		}
	    }
	}
	return max;
    }

    public void lakeMaker(int d){
	for(int r = 0; r < pasture.length; r++){
	    for(int c = 0; c < pasture[0].length; c++){
		if(pasture[r][c] > d){
		    pasture[r][c] = 0;
		} else {
		    pasture[r][c] = d - pasture[r][c];
		}
	    }
	}
    }

}
	  

